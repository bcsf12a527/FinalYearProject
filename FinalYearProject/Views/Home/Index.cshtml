                    <div class="clearfix"></div>
                    <div class="col-md-12 align-c pull-down-62 hs_portfolio index_portfolio">
                        <!-- /.title -->
                        <h4 class="hs_heading">Available Doctors</h4>
                        <ul class="push-down-62 portfolio-filter" data-scrollreveal="enter from the bottom over 1s but wait 0.5s">
                            <li><a href="profile-single.html" class="filter-item active" data-filter="all">All Team</a></li>
                            @foreach (var item in ViewBag.Category)
                            {
                                <li><a href="profile-single.html" class="filter-item" data-filter=@item>@item</a></li>
                            }
                        </ul>
                        <!-- /.push-down-62 portfolio-filter -->
                    </div>
                    <div class="clearfix"></div>
                    <!-- /.col-md-4 portfolio-item mix -->
                    <div class="row portfolio-grid" id="grid">
                        @foreach (Doctor_vs_Category item in ViewBag.Doctors)
                        {
                            <div class="col-md-4 col-sm-6 portfolio-item @item.Category mix">
                                <img onclick="location.href ='@Url.Action("DoctorProfile", "Doctor", new { Id = item.Id})'" src=@item.Image_Path alt="" width="380" height="400" />
                                <div class="portfolio_doctor_details">
                                    <h4> Dr. @item.Name</h4>
                                    <fieldset class="rate">
                                        @for (int i = 1; i <= @item.Rating; i++)
                                        {
                                            <input type="radio" id="rating" name="rating" /><label class="full" for="rating"></label>
                                        }
                                        @if (@item.Rating % 1 >= 0.5)
                                        {
                                            <input type="radio" id="rating" name="rating" /><label class="half" for="rating"></label>
                                        }
                                    </fieldset>
                                    <br />
                                    <a class="btn btn-success">@item.Category Specialist</a>
                                </div>
                            </div>
                        }
                        <!-- /.col-md-4 portfolio-item mix -->
                    </div>
                </div>
            </div>
        </div>

